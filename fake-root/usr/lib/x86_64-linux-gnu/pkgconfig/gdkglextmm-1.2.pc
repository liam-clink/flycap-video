prefix=/usr
exec_prefix=${prefix}
libdir=${prefix}/lib/x86_64-linux-gnu
includedir=${prefix}/include

target=x11
api_version=1.2

multihead=yes

Name: gdkglextmm
Description: C++ Wrapper for GdkGLExt (${target} target)
Version: 1.2.0
Requires: gdkglext-1.0 gdkmm-2.4 pangomm-1.4
Libs: -L${libdir} -lgdkglextmm-${target}-1.2 
Cflags: -I${includedir}/gtkglextmm-${api_version} -I${libdir}/gtkglextmm-${api_version}/include 
